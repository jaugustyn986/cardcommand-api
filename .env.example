# Database
DATABASE_URL="postgresql://username:password@localhost:5432/cardcommand?schema=public"

# Redis (for caching and sessions)
REDIS_URL="redis://localhost:6379"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"
JWT_EXPIRES_IN="7d"

# Server
PORT=3001
NODE_ENV="development"

# Frontend URL (for CORS)
FRONTEND_URL="http://localhost:5173"

# Release scrape extraction (optional; enables product-level data from IGN etc.)
OPENAI_API_KEY=""
OPENAI_EXTRACTION_MODEL="gpt-4o-mini"

# eBay API (optional)
EBAY_APP_ID=""
EBAY_CERT_ID=""
EBAY_DEV_ID=""
EBAY_AUTH_TOKEN=""
